function ABCheck(str) {  
  var string = str.split("");
  for (var i = 0; i<string.length; i++) {
    if (string[i]=='a' && string[i+4]== 'b') {
             return true;
    }
             
    else if (string[i]=='b' && string[i+4]== 'a') {
            return true;
    }

    else return false;
    
    }
}
